        <rdfs:subClassOf rdf:resource="https://dahcc.idlab.ugent.be/Ontology/ActivityRecognition/Model"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://dahcc.idlab.ugent.be/Ontology/ActivityRecognition/predicts"/>
